<style type="text/css">
.alerts-container{ background: none; position: absolute; right: 0; height: 500px; bottom: 0; z-index: 5000000; width: 400px; }  
.alert-box {min-height: 80px; width: 400px; position: fixed; bottom: 0; padding: 10px; color: #FFF; border-radius: 0px; margin-bottom: 0;}
.alert-header h2{ padding: 0; margin: 0; padding-bottom: 10px; font-weight: bold; font-size: 15px;}

.alert-box a.close{ position: relative; float: right; right: 0; top: -28px; font-size: 15px; opacity: 0.2;}
.actions-container .btn{ float: right;}

</style>

</head>

<body>
  <div id="wrapper">
    <div id="sidebar">
      
      <% 
      include("../shared/_side_nav.jag") 
      %>
    </div>
    
    <div id="page-wrapper" class="gray-bg">
      <div id="header">
        <% 
        include("../shared/_header.jag") 
        %>
      </div>
      
      <div>

        <%
          include('inner_layouts/_' + data.content_layout + '.jag') 
        %>

        <%
        //include("../shared/_summary_slider.jag") 
        %>
        
        <div style="height: 100px; margin-bottom: 100px;"></div>
      </div>
      
      <div id="footer">
        <%
        include("../shared/_footer.jag") 
        %>
      </div>
    </div>
    
    <div id="alert_bar">
      <%
      include("../shared/_alerts_container.jag") 
      %>
    </div>


    





  </div>


<div class="alerts-container hide" id="alertsContainer" style="" >
